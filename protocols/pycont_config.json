{
    "io": {
        "port": "/dev/tty.usbserial-FT2ISF2K",
        "baudrate": 9600,
        "timeout": 3
    },
    "default": {
        "volume": 5,
        "micro_step_mode": 2,
        "top_velocity": 6000,
        "initialize_valve_position": "I"
    },
    "groups": {
        "cleaning": ["sample", "water", "acetone"]
    },
    "pumps": {
        "sample": {
            "switch": "0",
            "volume": 12.5,
            "top_velocity":3000
            
        },

        "acetone": {
            "switch": "1",
            "volume": 5
            
        },

        "water": {
            "switch": "2",
            "volume": 1,
            "top_velocity": 12000
        }
    }
}